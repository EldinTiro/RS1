
@{
    ViewData["Title"] = "Index";
}
@model Ispit_2017_09_11_DotnetCore.ViewModels.OdjeljenjeIndexVM
<h2>Index</h2>

<table class="table table-striped">
    <thead>
    <tr>
        <th>Skolska godina</th>
        <th>Razred(od 1 do 4)</th>
        <th>Oznaka</th>
        <th>Razrednik</th>
        <th>Prebaceni u visi razred(odjeljenje)</th>
        <th>Akcija</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var x in Model.Rows)
    {
        <tr>
            <td>@x.SkolskaGodina</td>
            <td>@x.Razred</td>
            <td>@x.Oznaka</td>
            <td>@x.Razrednik</td>
            <td>@x.Prebacen</td>
            <td>
                
            </td>
        </tr>
    }
    </tbody>
</table>